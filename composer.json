{
    "name": "programster/async-curl",
    "type": "library",
    "description": "A package to make it easier to send curl request asynchronously using [curl_multi_exec](https://secure.php.net/manual/en/function.curl-multi-exec.php).",
    "keywords": ["curl"],
    "homepage": "https://github.com/programster/curl-easier",
    "license": "MIT",
    "authors": [
        {
            "name": "Stuart Page",
            "email": "sdpagent@gmail.com",
            "homepage": "https://stuart.page",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Programster\\AsyncCurl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Programster\\AsyncCurl\\Testing\\": "testing",
            "Programster\\AsyncCurl\\Testing\\Tests\\": "testing/tests"
        }
    }
}